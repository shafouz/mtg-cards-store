<div class="row card-images">
        <% @products.each do |product| %>
          <div class="col-md-4 my-2">
            <div class="card h-100">
              <a href="#"><%= image_tag(product.image_uris, class: "card-img-top")%></a>
                <div class="card-body">
                  <h4 class="card-title">
                  <a href="#"><%= product.name %></a>
                  </h4>
                  <h5>$24.99</h5>
                </div>
            </div>
          </div>
        <% end %>
</div>